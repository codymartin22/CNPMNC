@{
    ViewData["Title"] = "Login";
}
@model IEnumerable<String> 
<h1>External Login</h1>
<hr />
@if (Model.Count() == 0)
{
    <h5>No external login configured</h5>
}
else
{
    foreach (var item in Model)
    {
         <a class="btn btn-primary login-button" asp-action="SignIn" asp-route-provider="@item">Login with @item</a>
    }
}